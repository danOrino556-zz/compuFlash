<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.trivia.compuflash.app" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.trivia.compuflash.app.ActivitySplashOne"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.trivia.compuflash.app.ActivitySplashTwo"
            android:label="@string/title_activity_activity_splash_two" >
        </activity>
        <activity
            android:name="com.trivia.compuflash.app.ActivityMain"
            android:label="@string/title_activity_activity_main" >
        </activity>
        <activity
            android:name="com.trivia.compuflash.app.ActivityFlashCard"
            android:label="@string/title_activity_activity_flash_card" >
        </activity>
    </application>


    <uses-permission android:name="android.permission.INTERNET" />

</manifest>
